# Example environment file for frontend
# Copy to .env and update the URL to point to external backend

# For local development (if backend runs on same machine):
# REACT_APP_API_URL=http://localhost:8008/api/v1

# For VPS deployment (backend running on different machine):
REACT_APP_API_URL=http://148.230.105.139:8008/api/v1
# Example: REACT_APP_API_URL=http://148.230.105.139:8008/api/v1

# For NGROK/hosted endpoints:
# REACT_APP_API_URL=https://abcd-12-34-56-78.ngrok.io/api/v1

# If REACT_APP_API_URL is not set, the frontend will use relative paths under /api/v1
# (only works when frontend is proxied to the backend via reverse proxy)

REACT_APP_API_URL=

# Supabase credentials (required)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# Optional: a specific rebuild API for the rebuilt chat component
# REACT_APP_REBUILD_API_URL=
